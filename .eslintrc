{
    "env": {
        "browser": true,
        "node": true,
        "mocha": true,
        "es6": true
    },
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "rules": {
        "brace-style": [2, "1tbs"],
        "block-scoped-var": 2,
        "camelcase": 2,
        "complexity": [0, 4],
        "curly": 2,
        "dot-notation": 2,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "max-depth": [0, 4],
        "max-len": [0, 80, 4],
        "max-params": [0, 3],
        "max-statements": [0, 10],
        "new-cap": 2,
        "new-parens": 2,

        "no-alert": 2,
        "no-array-constructor": 2,
        "no-bitwise": 2,
        "no-caller": 2,
        "no-catch-shadow": 2,
        "no-console": 0,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-div-regex": 2,
        "no-dupe-keys": 2,
        "no-else-return": 2,
        "no-empty": 2,
        "no-empty-class": 2,
        "no-empty-label": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-implied-eval": 2,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-loop-func": 2,
        "no-mixed-requires": [0, false],
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-wrappers": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-plusplus": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-shadow": 2,
        "no-sync": 2,
        "no-ternary": 0,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-underscore-dangle": 2,
        "no-unreachable": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "no-with": 2,
        "no-wrap-func": 2,

        "one-var": 2,
        "quote-props": 0,
        "quotes": [2, "single"],
        "radix": 0,
        "semi": 2,
        "space-after-keywords": [2, "always"],
        "space-before-blocks": [2, "always"],
        "space-before-function-parentheses": [2, {"anonymous": "always", "named": "never"}],
        "space-in-parens": [2, "never"],
        "strict": [2, "global"],
        "use-isnan": 2,
        "valid-typeof": 0,
        "wrap-iife": 2,
        "wrap-regex": 0,
        "yoda": [2, "always"]

    }
}